{
  "manifest_version": 3,
  "name": "Cargopolo Calculator",
  "version": "1.0.0",
  "description": "Calculator extension for Cargopolo",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["assets/*"],
      "matches": ["<all_urls>"]
    }
  ]
}

